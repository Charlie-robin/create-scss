{
  "name": "create-scss",
  "version": "1.1.5",
  "description": "A starter template with scss directory structure, dependencies, script for developement and production",
  "main": "index.js",
  "dependencies": {
    "autoprefixer": "^9.7.4",
    "cssnano": "^4.1.10",
    "node-sass": "^4.13.0",
    "postcss-cli": "^7.1.0"
  },
  "devDependencies": {},
  "scripts": {
    "watch": "node-sass scss/main.scss css/style.css --watch",
    "compile": "node-sass scss/main.scss css/style.css",
    "prefix": "postcss css/style.css --use autoprefixer -b 'last 2 versions' -o css/style.css",
    "compress": "postcss --use cssnano --output css/style.mini.css css/style.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maximedaraize/create-scss"
  },
  "keywords": [
    "template",
    "css",
    "scss",
    "sass",
    "starter",
    "directory",
    "structure"
  ],
  "author": "Maxime Daraize",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/maximedaraize/create-scss/issues"
  },
  "homepage": "https://github.com/maximedaraize/create-scss#readme"
}
